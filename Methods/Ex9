package assignment;

public class Ex9 {
	public static void main(String[] args) {
		double length = 5.0;
		double width = 3.0;
		double area = calculateRectangleArea(length, width);
		
		System.out.println(area);
	}
	public static double calculateRectangleArea(double length , double width) {
		return length * width;
		
	}

}
