package assignment;

import java.util.Scanner;

public class Ex11 {
	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		int a = sc.nextInt();
		int result = squareInt(a);
		System.out.println(result);
		
	}
	
	public static int squareInt(int value) {
		int integer = (value * value);
		return integer;
	}

}
